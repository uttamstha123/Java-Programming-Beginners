package StudentDatabase;

public class StudentDatabase {
	// Variables
	String name;
	String ssn;
	int id;
	String userId;
	private long phno;
	private String city;
	private String state;

	// Constructor to take name and ssn
	StudentDatabase(String name, String ssn) {
		this.name = name;
		this.ssn = ssn;
	}

	void generateEmail() {
		String email = name.toLowerCase() + "" + Math.random() * 100 + "" + "@gmail.com";
		System.out.println("Email : " + email);
	}
}
